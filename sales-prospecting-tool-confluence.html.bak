            function extractSegmentFromDealName(dealName) {
                const match = dealName.match(/^(.+?)\s+Purchase Intender/);
                return match ? match[1].trim() : '';
            }

            // Fuzzy match segment names
            function matchesSegment(dealSegmentName, targetSegment) {
                if (!dealSegmentName || !targetSegment) return false;
                
                const dealLower = dealSegmentName.toLowerCase();
                const targetLower = targetSegment.toLowerCase();
                
                // Exact match
                if (dealLower === targetLower) return true;
                
                // Plural/singular variations
                if (dealLower === targetLower + 's' || dealLower + 's' === targetLower) return true;
                
                // Contains match (for multi-word segments)
                if (dealLower.includes(targetLower) || targetLower.includes(dealLower)) return true;
                
                // Handle "Pet Owners" vs "Pet Supplies"
                if (dealLower.includes('pet') && targetLower.includes('pet')) return true;
                if (dealLower.includes('baby') && targetLower.includes('baby')) return true;
                
                return false;
            }

            const relevantDeals = [];
            const seen = new Set();

            // Find deals matching primary segment or overlap product
            commerceDeals.forEach(deal => {
                const dealSegment = extractSegmentFromDealName(deal.dealName);
                const matchesPrimary = matchesSegment(dealSegment, segment);
                const matchesOverlap = matchesSegment(dealSegment, overlapProduct);
                
                if (matchesPrimary || matchesOverlap) {
                    console.log(`  ‚úì Match: ${dealSegment} ‚Üí ${deal.dealId}`);
                }
                
                if ((matchesPrimary || matchesOverlap) && !seen.has(deal.dealId)) {
                    relevantDeals.push({
                        dealId: deal.dealId,
                        dealName: deal.dealName.replace(' Purchase Intender', ''),
                        targeting: dealSegment,
                        bidGuidance: deal.bidGuidance || 'Contact for pricing',
                        matchScore: matchesPrimary && matchesOverlap ? 3 : (matchesPrimary ? 2 : 1)
                    });
                    seen.add(deal.dealId);
                }
            });

            // Sort by match score (highest first) and return top 5
            relevantDeals.sort((a, b) => b.matchScore - a.matchScore);
            const topDeals = relevantDeals.slice(0, 5);
            
            console.log(`‚úÖ Found ${relevantDeals.length} relevant deals, showing top ${topDeals.length}`);
            
            return topDeals;
        }
        
        function downloadCard() {
            const svg = document.getElementById('generated-card');
            const svgData = new XMLSerializer().serializeToString(svg);
            
            const canvas = document.createElement('canvas');
            canvas.width = 350;
            canvas.height = 450;
            const ctx = canvas.getContext('2d');
            
            const img = new Image();
            const svgBlob = new Blob([svgData], { type: 'image/svg+xml;charset=utf-8' });
            const url = URL.createObjectURL(svgBlob);
            
            img.onload = function() {
                ctx.fillStyle = 'white';
                ctx.fillRect(0, 0, 350, 450);
                ctx.drawImage(img, 0, 0);
                
                canvas.toBlob(function(blob) {
                    const link = document.createElement('a');
                    link.href = URL.createObjectURL(blob);
                    const filename = `${selectedSegment.replace(/\s+/g, '-')}-${audienceData[selectedSegment].overlaps[selectedOverlap].name.replace(/\s+/g, '-')}-Audience.jpg`;
                    link.download = filename;
                    document.body.appendChild(link);
                    link.click();
                    document.body.removeChild(link);
                    URL.revokeObjectURL(link.href);
                }, 'image/jpeg', 0.95);
                
                URL.revokeObjectURL(url);
            };
            
            img.src = url;
        }
        
        function copyEmail() {
            const email = document.getElementById('email-template').textContent;
            navigator.clipboard.writeText(email).then(() => {
                const btn = document.getElementById('copy-email-btn');
                const originalText = btn.textContent;
                btn.textContent = '‚úÖ Copied to Clipboard!';
                setTimeout(() => {
                    btn.textContent = originalText;
                }, 2000);
            });
        }
    </script>
