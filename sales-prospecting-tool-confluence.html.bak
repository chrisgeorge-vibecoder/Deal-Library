        function wrapText(text, x, startY, fontSize, lineHeight, maxWidth) {
            const words = text.split(' ');
            let lines = [];
            let currentLine = [];
            
            words.forEach(word => {
                currentLine.push(word);
                const testLine = currentLine.join(' ');
                if (testLine.length > 50) {
                    currentLine.pop();
                    lines.push(currentLine.join(' '));
                    currentLine = [word];
                }
            });
            if (currentLine.length > 0) {
                lines.push(currentLine.join(' '));
            }
            
            return lines.map((line, i) => 
                `<text x="${x}" y="${startY + (i * lineHeight)}" font-family="system-ui, sans-serif" font-size="${fontSize}" fill="#282828" text-anchor="middle">${line}</text>`
            ).join('\n');
        }
        
        function generateInsight(segment, overlapProduct) {
            // Generate concise insights that connect the two products (max ~200 chars for card)
            const lowerSegment = segment.toLowerCase();
            const lowerProduct = overlapProduct.toLowerCase();
            
            // Create connecting insights based on the overlap
            let insight = '';
            
            // Tech + Lifestyle patterns
            if (lowerProduct.includes('laptop') || lowerProduct.includes('computer')) {
                insight = `${segment} buyers also invest in tech to track, organize, or enhance their lifestyle. Target with productivity and data-driven appeals.`;
            }
            // Household supplies connections
            else if (lowerProduct.includes('household') || lowerProduct.includes('supplies')) {
                insight = `${segment} shoppers value quality home essentials. They're organized buyers who invest across categories for their complete lifestyle.`;
            }
            // Apparel/Athletic connections
            else if (lowerProduct.includes('shoe') || lowerProduct.includes('athletic') || lowerProduct.includes('outerwear')) {
                insight = `Active ${segment} consumers also prioritize performance apparel. They're complete lifestyle buyers seeking quality across all gear.`;
            }
            // Sporting goods connections
            else if (lowerProduct.includes('sport') || lowerProduct.includes('recreation')) {
                insight = `${segment} buyers pursue active lifestyles. The overlap shows they're building complete gear collections for their adventures.`;
            }
            // Alcohol/Beverage connections
            else if (lowerProduct.includes('alcohol') || lowerProduct.includes('beverage')) {
                insight = `${segment} shoppers enjoy premium experiences. They invest in quality beverages as part of their lifestyle enjoyment rituals.`;
            }
            // Furniture/Home connections
            else if (lowerProduct.includes('furniture') || lowerProduct.includes('bed') || lowerProduct.includes('sofa')) {
                insight = `${segment} buyers are in home investment mode. They're making multiple purchases to create complete, cohesive living spaces.`;
            }
            // Food connections
            else if (lowerProduct.includes('food') || lowerProduct.includes('baby')) {
                insight = `${segment} shoppers prioritize nutrition and safety. They're careful buyers researching quality across all their purchases.`;
            }
            // Electronics/Accessories
            else if (lowerProduct.includes('electronic') || lowerProduct.includes('accessories')) {
                insight = `Tech-savvy ${segment} buyers build complete ecosystems. They research extensively and value compatible, quality products.`;
            }
            // Entertainment/Media
            else if (lowerProduct.includes('entertainment') || lowerProduct.includes('media') || lowerProduct.includes('toy')) {
                insight = `${segment} buyers invest in experiences and enjoyment. They value quality entertainment as part of their lifestyle.`;
            }
            // Office/Business
            else if (lowerProduct.includes('office')) {
                insight = `${segment} shoppers are productivity-focused. They invest in quality tools across work and personal life for efficiency.`;
            }
            // Default connection
            else {
                insight = `${segment} buyers also purchase ${overlapProduct}. These complementary behaviors reveal lifestyle alignment‚Äîtarget with bundled campaigns.`;
            }
            
            // Ensure it's under 200 characters
            if (insight.length > 200) {
                insight = insight.substring(0, 197) + '...';
            }
            
            return insight;
        }
        
